{
  "name": "reaviz",
  "version": "15.15.3",
  "description": "Data Visualization using React",
  "scripts": {
    "build": "vite build --mode library",
    "build-storybook": "storybook build",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "test": "vitest --passWithNoTests",
    "test:ci": "vitest run --coverage --passWithNoTests",
    "ts:check": "tsc --noEmit",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "storybook dev -p 9009",
    "prepare": "husky install"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reaviz.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "d3.js",
    "d3",
    "dataviz",
    "charts",
    "charting",
    "svg",
    "bar-chart",
    "axis",
    "brush",
    "scatter-plot",
    "area-chart",
    "sparkline",
    "line-chart",
    "pie-chart",
    "sankey",
    "gauge",
    "donut-chart",
    "pan",
    "zoom",
    "radial",
    "brush",
    "heatmap",
    "calendar-heatmap",
    "legends",
    "venn",
    "euler",
    "venn-diagram",
    "treemap"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reaviz/issues"
  },
  "homepage": "https://github.com/reaviz/reaviz#readme",
  "dependencies": {
    "@upsetjs/venn.js": "^1.3.0",
    "big-integer": "^1.6.52",
    "chroma-js": "^2.4.2",
    "classnames": "^2.5.1",
    "d3-array": "^3.0.4",
    "d3-format": "^3.0.1",
    "d3-geo": "^3.0.1",
    "d3-hierarchy": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.0.1",
    "d3-time": "^3.0.0",
    "ellipsize": "^0.5.1",
    "framer-motion": "^10.16.16",
    "human-format": "^1.2.0",
    "invert-color": "^2.0.0",
    "memoize-bind": "^1.0.3",
    "memoize-one": "^6.0.0",
    "reablocks": "^7.5.0",
    "react-cool-dimensions": "^3.0.1",
    "react-fast-compare": "^3.2.1",
    "safe-identifier": "^0.4.2",
    "transformation-matrix": "^2.9.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@mdx-js/react": "2.3.0",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-mdx-gfm": "^7.6.17",
    "@storybook/addon-storysource": "^7.6.17",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/theming": "^7.6.17",
    "@types/classnames": "^2.3.1",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-hierarchy": "^3.1.6",
    "@types/d3-sankey": "^0.12.4",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-shape": "^3.1.6",
    "@types/d3-time": "^3.0.3",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/topojson-client": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "moment": "^2.30.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.4.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^7.6.17",
    "topojson-client": "^3.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1",
    "world-atlas": "^2.0.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
